<h1>Patient: <%=@patient.name%></h1>
<h2>Appointments:</h2>
<%= @patient.appointments.each do |appointment| %> 
    <ul>
        <li>Doctor:<%= link_to appointment.doctor_name, doctor_path(appointment.doctor)%><br>
        <%= appointment.appointment_datetime.strftime('%B %d, %Y' + ' at ' "%H:%M") %></li>
    </ul>
<%end%>
